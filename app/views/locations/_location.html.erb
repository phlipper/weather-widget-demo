<div id="<%= dom_id location %>">
  <p>
    <strong>City:</strong>
    <%= location.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= location.state %>
  </p>

  <p>
    <strong>Postal code:</strong>
    <%= location.postal_code %>
  </p>

  <p>
    <strong>Lat:</strong>
    <%= location.lat %>
  </p>

  <p>
    <strong>Lng:</strong>
    <%= location.lng %>
  </p>

</div>
